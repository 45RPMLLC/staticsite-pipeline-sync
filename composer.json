{
    "name": "fortyfive/static-site-pipeline-sync",
    "description": "Gives Craft CMS the ability to trigger AWS CodeBuild projects to update the content of static sites. (where the content source is of course Craft CMS)",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "static-site",
        "pipeline",
        "aws-codebuild",
        "pipeline-sync",
        "static-site-pipeline-sync",
        "publication-workflow"
    ],
    "support": {
        "docs": "https://github.com/45RPMLLC/staticsite-pipeline-sync/static-site-pipeline-sync/blob/master/README.md",
        "issues": "https://github.com/45RPMLLC/staticsite-pipeline-sync/static-site-pipeline-sync/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "45RPM",
            "homepage": "https://www.45rpm.co/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "aws/aws-sdk-php": "3.100.*",
        "czproject/git-php": "3.17.0"
    },
    "autoload": {
        "psr-4": {
          "fortyfive\\staticsitepipelinesync\\": "src/"
        }
    },
    "extra": {
        "name": "Static Site Pipeline Sync",
        "handle": "static-site-pipeline-sync",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/45RPMLLC/staticsite-pipeline-sync /static-site-pipeline-sync/master/CHANGELOG.md",
        "class": "fortyfive\\staticsitepipelinesync\\StaticSitePipelineSync"
    }
}
